{
  "expo": {
    "name": "Tasks",
    "slug": "tasks",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.2",
    "orientation": "portrait",
    "userInterfaceStyle":"automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen":true,
      "userInterfaceStyle":"automatic"
    },
    "android":{
      "userInterfaceStyle":"automatic"
    },
    "androidStatusBar": {
      "hidden":true
    }
  }
}
